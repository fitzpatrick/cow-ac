<?xml encoding="US-ASCII"?>

	<!ELEMENT ScanTypeTable (header, body)>
	<!ELEMENT header (description?, remark*)>
	<!ATTLIST header
			version CDATA #REQUIRED
			maxnum	CDATA #REQUIRED
			num		CDATA #IMPLIED
			size	CDATA #IMPLIED
	>

	<!ELEMENT description (#PCDATA)>
	<!ELEMENT remark (#PCDATA)>

	<!ELEMENT body (entry)*>
	<!ELEMENT entry (radcode, radbcode, remarks)>

	<!ELEMENT radcode (#PCDATA)>
	<!ELEMENT radbcode (#PCDATA)>
	<!ELEMENT remarks (#PCDATA)>
