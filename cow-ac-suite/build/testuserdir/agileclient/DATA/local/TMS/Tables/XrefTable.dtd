<?xml encoding="US-ASCII"?>

	<!ELEMENT XrefTable (header, body)>
	<!ELEMENT header (description?, remark*)>
	<!ATTLIST header
			version CDATA #REQUIRED
			maxnum  CDATA #REQUIRED
			num		CDATA #IMPLIED
			size	CDATA #IMPLIED

	>

	<!ELEMENT description (#PCDATA)>
	<!ELEMENT remark (#PCDATA)>

	<!ELEMENT body (entry)*>
	<!ELEMENT entry (sid, fotc, command, xref)>

	<!ELEMENT sid (#PCDATA)>
	<!ELEMENT fotc (#PCDATA)>
	<!ELEMENT command (#PCDATA)>
	<!ELEMENT xref (#PCDATA)>
